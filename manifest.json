{
    "name": "Ext2",
    "version": "1.0",
    "description": "for personal use",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "run_at": "document_end",
            "all_frames": true
        }
    ],
    "web_accessible_resources": [{
        "resources": ["popup.png"],
        "matches": ["<all_urls>"]
      }],
    "permissions": ["scripting"],
    "host_permissions": ["<all_urls>"]
}
